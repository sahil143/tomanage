    "typescript": "^5.7.2"
  },
  "keywords": [
    "ticktick",
    "sdk",
    "typescript",
    "oauth",
    "task-management",
    "todo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tomanage",
    "directory": "packages/ticktick-sdk"
  },
  "license": "MIT"
