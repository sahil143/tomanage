{
  "name": "@tomanage/ticktick-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for TickTick API with OAuth support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
